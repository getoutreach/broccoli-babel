{
  "name": "broccoli-babel",
  "version": "0.0.1",
  "description": "Simple Babel filter with fast incremental recompiles",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "rm -rf lib/ && babel src -d lib"
  },
  "keywords": [
    "broccoli",
    "babel"
  ],
  "author": "Gordon L. Hempton <ghempton@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "broccoli-persistent-filter": "^1.2.11"
  }
}
